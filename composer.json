{
    "name": "peixinho/lazymephp",
    "description": "LazyMePHP is a PHP framework designed for rapid development with a focus on class, form, and REST API generation from database tables.",
    "type": "project",
    "keywords": ["php", "framework", "rest", "api", "orm", "database", "rad"],
    "homepage": "https://github.com/Peixinho/LazyMePHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Duarte Peixinho",
            "homepage": "https://github.com/Peixinho",
            "role": "Original Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/Peixinho/LazyMePHP/issues",
        "source": "https://github.com/Peixinho/LazyMePHP"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "eftec/bladeone": "^4.13",
        "pecee/simple-router": "^5.4",
        "adhocore/jwt": "^1.1.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "platform-check": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "App\\Api\\": "App/Api/",
            "Models\\": "App/Models/",
            "Controllers\\": "App/Controllers/",
            "Core\\": "App/Core/",
            "Core\\DB\\": "App/Core/DB/",
            "Core\\Enum\\": "App/Core/Enum/",
            "Core\\Helpers\\": "App/Core/Helpers/",
            "Messages\\": "App/Messages/",
            "Routes\\": "App/Routes/",
            "Core\\Security\\": "App/Core/Security/",
            "Core\\Session\\": "App/Core/Session/",
            "Tools\\": "App/Tools/",
            "Core\\Validations\\": "App/Core/Validations/",
            "Views\\": "App/Views/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "phpstan/phpstan": "^2.1"
    }
}
